{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/products",
        "dest": "index.js"
      },
      {
        "src": "/products/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/brands",
        "dest": "index.js"
      },
      {
        "src": "/brands/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/categorys",
        "dest": "index.js"
      },
      {
        "src": "/categorys/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/users",
        "dest": "index.js"
      },
      {
        "src": "/users/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/auth",
        "dest": "index.js"
      },
      {
        "src": "/auth/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/cart",
        "dest": "index.js"
      },
      {
        "src": "/cart/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/orders",
        "dest": "index.js"
      },
      {
        "src": "/orders/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/",
        "dest": "build/index.html"
      },
      {
        "src": "/login",
        "dest": "build/index.html"
      },
      {
        "src": "/react/cart",
        "dest": "build/index.html"
      },
      {
        "src": "/checkout",
        "dest": "build/index.html"
      },
      {
        "src": "/product-detail",
        "dest": "build/index.html"
      },
      {
        "src": "/myprofile",
        "dest": "build/index.html"
      },
      {
        "src": "/myorders",
        "dest": "build/index.html"
      },
      {
        "src": "/admin",
        "dest": "build/index.html"
      },
      {
        "src": "/admin/detail/product-detail/",
        "dest": "build/index.html"
      },
      {
        "src": "/admin/product-form",
        "dest": "build/index.html"
      },
      {
        "src": "/admin/product-form/edite/",
        "dest": "build/index.html"
      },
      {
        "src": "/admin/order",
        "dest": "build/index.html"
      },
      {
        "src": "/orderSucces/",
        "dest": "build/index.html"
      },
      {
        "src": "*",
        "dest": "build/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "build/$1"
      }
    ]
  }

